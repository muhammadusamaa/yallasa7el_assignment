<h3 class="login_id">Yalla & Locate your vacation's destination: </h3> 
<div class="nav">
	<%= form_for Property.new, :html => { :method => 'GET', id: 'search_property' }, :url => :properties_search, remote: true do |f| %>
  		<div class="search_list"><%= f.text_field :destination, placeholder: 'Destination' %></div><br>
  		<div class="search_list"><%= f.number_field :number_of_rooms, placeholder: 'Number of Rooms' %></div>
  		<%= f.submit 'Search', id: 'search_button', onClick: 'javascript:submitForm();' %>
	<% end %>
</div>




<div id="display_result"></div>

